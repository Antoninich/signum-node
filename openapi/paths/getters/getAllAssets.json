{
  "get": {
    "summary": "Get All Tokens",
    "description": "Get all tokens, but at maximum 500 per call.",
    "tags": [
      "token"
    ],
    "parameters": [
      {
        "$ref": "../../parameters/firstIndex.json"
      },
      {
        "$ref": "../../parameters/lastIndex.json"
      },
      {
        "name": "quantityMinimumQNT",
        "in": "query",
        "description": "Count only accounts with given minimum quantity.. This parameter affects the `numberOfAccounts` in the response. This helps you to calculate the fees upfront for [Distribution to Token  holders](#post-/api-requestType-distributeToAssetHolders)<br/>❕️This parameter __does not filter__ the result!",
        "schema": {
          "type": "integer",
          "format": "int64"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "The Token List",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "assets"
              ],
              "properties": {
                "assets": {
                  "type": "array",
                  "items": {
                    "$ref": "../../schemas/asset.json"
                  }
                }
              }
            }
          }
        }
      },
      "500": {
        "$ref": "../../responses/error.json"
      }
    }
  }
}
